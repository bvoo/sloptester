    "lucide-vue-next": "^0.475.0",
    "radix-vue": "^1.9.16",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tauri-apps/cli": "^2.0.0-beta.0",
    "@types/node": "^22.13.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vueuse/core": "^12.7.0",
    "tailwindcss": "^4.0.7",
